SELECT 
DATEDIFF(YEAR, '2000/01/23', GETDATE()) AS Лет,
DATEDIFF(MONTH, '2000/01/23', GETDATE()) AS Месяцев,
DATEDIFF(WEEK, '2000/01/23', GETDATE()) AS Недель,
DATEDIFF(DAY, '2000/01/23', GETDATE()) AS Дней,
DATEDIFF(HOUR, '2000/01/23', GETDATE()) AS Часов,
DATEDIFF(MINUTE, '2000/01/23', GETDATE()) AS Минут,
DATEDIFF(SECOND, '2000/01/23', GETDATE()) AS Секунд
